    <script>
      var hiJusticeText = document.getElementById("hiJusticeText");
      if (hiJusticeText) {
        hiJusticeText.addEventListener("click", function () {
          var anchor = document.querySelector("[data-scroll-to='userImage']");
          if (anchor) {
            anchor.scrollIntoView({ block: "start", behavior: "smooth" });
          }
        });
      }
      
      var timetableContainer = document.getElementById("timetableContainer");
      if (timetableContainer) {
